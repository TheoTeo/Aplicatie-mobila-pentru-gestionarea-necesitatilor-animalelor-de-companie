<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Licenta01" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#DF572D</item>
        <item name="colorPrimaryVariant">#FFD9A2</item>
        <item name="colorOnPrimary">#FFFFFF</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#DF572D</item>
        <item name="colorSecondaryVariant">#DF572D</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="textViewStyle">
        <item name="android:textSize">30dp</item>
        <item name="font">@font/adamina</item>
        <item name="fontFamily">@font/adamina</item>
    </style>


    <style name="Theme.Licenta01.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Licenta01.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Licenta01.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


</resources>